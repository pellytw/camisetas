<h2>Registrar:</h2>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <%= render :partial => 'user_fields' , :locals => {:f => f} %>

  <div><%= f.submit "Registrar" %></div>
<% end %>

<%= render "/devise/shared/links.erb" %>
