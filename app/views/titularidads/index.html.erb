<h1>Listado de Titularidades</h1>

<table class="table table-striped">
  <tr>
    <th>Nombre</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @titularidads.each do |titularidad| %>
  <tr>
    <td><%= titularidad.nombre %></td>
    <td><%= link_to 'Ver', titularidad, :class => "btn btn-small" %></td>
    <td><%= link_to 'Editar', edit_titularidad_path(titularidad), :class => "btn btn-small" %></td>
    <td><%= link_to 'Eliminar', titularidad, method: :delete, data: { confirm: 'Seguro desea eliminar?' }, :class => "btn btn-small" %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nueva Titularidad', new_titularidad_path, :class => "btn btn-success" %>
